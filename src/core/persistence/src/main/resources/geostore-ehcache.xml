<ehcache xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="ehcache.xsd">

	<diskStore path="java.io.tmpdir" />

	<!-- defaultCache maxElementsInMemory="10000" eternal="false"
		timeToIdleSeconds="120" timeToLiveSeconds="120" overflowToDisk="true"
		diskPersistent="false" diskExpiryThreadIntervalSeconds="120"
		memoryStoreEvictionPolicy="LRU" />

	<cache name="gs_stored_data" maxElementsInMemory="10000" eternal="false"
		timeToIdleSeconds="240" timeToLiveSeconds="240" overflowToDisk="true"
		diskPersistent="false" diskExpiryThreadIntervalSeconds="240"
		memoryStoreEvictionPolicy="LRU" />
		
	<cache name="gs_resource" maxElementsInMemory="10000" eternal="false"
		timeToIdleSeconds="240" timeToLiveSeconds="240" overflowToDisk="true"
		diskPersistent="false" diskExpiryThreadIntervalSeconds="240"
		memoryStoreEvictionPolicy="LRU" />
		
	<cache name="gs_attribute" maxElementsInMemory="10000" eternal="false"
		timeToIdleSeconds="240" timeToLiveSeconds="240" overflowToDisk="true"
		diskPersistent="false" diskExpiryThreadIntervalSeconds="240"
		memoryStoreEvictionPolicy="LRU" />
		
	<cache name="gs_security" maxElementsInMemory="10000" eternal="false"
		timeToIdleSeconds="240" timeToLiveSeconds="240" overflowToDisk="true"
		diskPersistent="false" diskExpiryThreadIntervalSeconds="240"
		memoryStoreEvictionPolicy="LRU" />
	
	<cache name="gs_category" maxElementsInMemory="10000" eternal="false"
		timeToIdleSeconds="240" timeToLiveSeconds="240" overflowToDisk="true"
		diskPersistent="false" diskExpiryThreadIntervalSeconds="240"
		memoryStoreEvictionPolicy="LRU" />
	
	<cache name="gs_user_attribute" maxElementsInMemory="10000" eternal="false"
		timeToIdleSeconds="240" timeToLiveSeconds="240" overflowToDisk="true"
		diskPersistent="false" diskExpiryThreadIntervalSeconds="240"
		memoryStoreEvictionPolicy="LRU" />
	
	<cache name="gs_user" maxElementsInMemory="10000" eternal="false"
		timeToIdleSeconds="240" timeToLiveSeconds="240" overflowToDisk="true"
		diskPersistent="false" diskExpiryThreadIntervalSeconds="240"
		memoryStoreEvictionPolicy="LRU" />
			
	<cache name="gs_usergroup" maxElementsInMemory="10000" eternal="false"
		timeToIdleSeconds="240" timeToLiveSeconds="240" overflowToDisk="true"
		diskPersistent="false" diskExpiryThreadIntervalSeconds="240"
		memoryStoreEvictionPolicy="LRU" />
		
	<cache
		name="org.hibernate.cache.StandardQueryCache"
		maxElementsInMemory="200"
		eternal="false"
		timeToLiveSeconds="120"
		diskPersistent="true"
		overflowToDisk="true"/>
	
	<cache
		name="org.hibernate.cache.UpdateTimestampsCache"
		maxElementsInMemory="5000"
		eternal="true"
		overflowToDisk="true"/>-->

</ehcache>