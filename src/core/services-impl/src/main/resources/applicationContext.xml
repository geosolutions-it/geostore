<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	   xmlns:context="http://www.springframework.org/schema/context"
	   xmlns:aop="http://www.springframework.org/schema/aop"
	   xsi:schemaLocation="
	    	http://www.springframework.org/schema/beans 
	   		http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
			http://www.springframework.org/schema/context
			http://www.springframework.org/schema/context/spring-context-2.5.xsd
			http://www.springframework.org/schema/aop 
			http://www.springframework.org/schema/aop/spring-aop-2.5.xsd"
       default-autowire="byName">

    <bean id="storedDataService" class="it.geosolutions.geostore.services.StoredDataServiceImpl">
    </bean>

    <bean id="resourceService" class="it.geosolutions.geostore.services.ResourceServiceImpl">
    </bean>
    
    <bean id="categoryService" class="it.geosolutions.geostore.services.CategoryServiceImpl">
    </bean>
    
    <bean id="userService" class="it.geosolutions.geostore.services.UserServiceImpl">
    </bean>
    
    <bean id="userGroupService" class="it.geosolutions.geostore.services.UserGroupServiceImpl">
    </bean>
    <bean id="userSessionService" class="it.geosolutions.geostore.services.InMemoryUserSessionServiceImpl">
    </bean>
    
</beans>
